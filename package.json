{
  "name": "ngx-toolset",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "build:pkg:api-token-interceptor": "ng b @ngx-toolset/api-token-interceptor",
    "build:pkg:api-token-interceptor:watch": "npm run build:api-token-interceptor -- --watch -c development",
    "build:pkg:date-interceptors": "ng b @ngx-toolset/date-interceptors",
    "build:pkg:date-interceptors:watch": "npm run build:date-interceptors -- --watch -c development",
    "build:pkg:lazy-dialogs": "ng b @ngx-toolset/lazy-dialogs",
    "build:pkg:lazy-dialogs:watch": "npm run build:lazy-dialogs -- --watch -c development",
    "build:pkg:loading-spinner": "ng b @ngx-toolset/loading-spinner",
    "build:pkg:loading-spinner:watch": "npm run build:loading-spinner -- --watch -c development",
    "build:pkg:template-type-checker": "ng b @ngx-toolset/template-type-checker",
    "build:pkg:template-type-checker:watch": "npm run build:template-type-checker -- --watch -c development",
    "test:unit:api-token-interceptor": "ng t @ngx-toolset/api-token-interceptor --browsers=ChromeHeadless",
    "test:unit:api-token-interceptor:watch": "npm run test:unit:api-token-interceptor -- --watch",
    "test:unit:date-interceptors": "ng t @ngx-toolset/date-interceptors --browsers=ChromeHeadless",
    "test:unit:date-interceptors:watch": "npm run test:unit:date-interceptors -- --watch",
    "test:unit:lazy-dialogs": "ng t @ngx-toolset/lazy-dialogs --browsers=ChromeHeadless",
    "test:unit:lazy-dialogs:watch": "npm run test:unit:lazy-dialogs -- --watch",
    "test:unit:loading-spinner": "ng t @ngx-toolset/loading-spinner --browsers=ChromeHeadless",
    "test:unit:loading-spinner:watch": "npm run test:unit:loading-spinner -- --watch",
    "test:unit:template-type-checker": "ng t @ngx-toolset/template-type-checker --browsers=ChromeHeadless",
    "test:unit:template-type-checker:watch": "npm run test:unit:template-type-checker -- --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.6",
    "@angular/cli": "~14.0.6",
    "@angular/compiler-cli": "^14.0.0",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^14.0.0",
    "typescript": "~4.7.2"
  }
}
