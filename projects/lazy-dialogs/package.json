{
  "name": "@ngx-toolset/lazy-dialogs",
  "version": "1.0.0-rc.10",
  "scripts": {
    "build": "tsc -p tsconfig.schematics.json",
    "postbuild": "copyfiles schematics/*/schema.json schematics/*/files/** schematics/collection.json ../../dist/lazy-dialogs/"
  },
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": "dependencies"
  },
  "description": "Angular lazy dialogs support via service",
  "homepage": "https://github.com/SwabianCoder/ngx-toolset",
  "license": "MIT",
  "author": "SwabianCoder",
  "keywords": [
    "angular",
    "ngx",
    "ngx-toolset",
    "lazy-dialogs",
    "service"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SwabianCoder/ngx-toolset"
  },
  "bugs": {
    "url": "https://github.com/SwabianCoder/ngx-toolset/issues"
  },
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@angular/common": "^16.0.0",
    "@angular/core": "^16.0.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "copyfiles": "file:../../node_modules/copyfiles",
    "typescript": "file:../../node_modules/typescript"
  }
